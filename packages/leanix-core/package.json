  "scripts": {
    "compile": "tsc && npm run generateSchema",
    "generateSchema": "npm run generateSchema:credentials && npm run generateSchema:metadata",
    "generateSchema:credentials": "npx typescript-json-schema tsconfig.json LeanIXCredentials --id /LeanIXCredentials  -o schema/LeanIXCredentials.json",
    "generateSchema:metadata": "npx typescript-json-schema tsconfig.json CustomReportMetadata --id /CustomReportMetadata  -o schema/CustomReportMetadata.json"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@types/node-fetch": "^2.5.12",
    "@types/tar": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "typescript": "^4.0.3",
    "typescript-json-schema": "^0.50.1"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "form-data": "^3.0.1",
    "jsonschema": "^1.4.0",
    "jwt-decode": "^3.1.2",
    "node-fetch": "^2.6.1",
    "tar": "^6.1.10"
  }
}
